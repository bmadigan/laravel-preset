{
    "name": "bmadigan/laravel-preset",
    "description": "Brads Laravel application preset",
    "keywords": [
        "bmadigan",
        "laravel-preset"
    ],
    "homepage": "https://github.com/bmadigan/laravel-preset",
    "license": "MIT",
    "authors": [
        {
            "name": "Brad Madigan",
            "email": "bradmadigan@gmail.com",
            "homepage": "https://dizzytree.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "laravel/framework": "^5.8",
        "nunomaduro/laravel-console-task": "^1.2",
        "sixlive/dotenv-editor": "^1.1"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "bmadigan\\LaravelPreset\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "bmadigan\\LaravelPreset\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "bmadigan\\LaravelPreset\\LaravelPresetServiceProvider"
            ]
        }
    }
}
